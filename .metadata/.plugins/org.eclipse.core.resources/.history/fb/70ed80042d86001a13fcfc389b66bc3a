package jAVA;

public class pyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int n=6;
		 for(int i=1;i<=n;i++)
		 {
			 for(int j=n-i; j>1; j--)
			 {
				 System.out.print(" ");
				 
			 }
			for (int j=1; j<=i; j++)
			{
		
				System.out.print(j);
			}
			System.out.println();
			
		 }	
		  
		 
	}

}
